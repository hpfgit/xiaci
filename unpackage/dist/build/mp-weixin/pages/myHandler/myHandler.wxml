<view class="my_handle"><view class="my_search"><view class="my_search_main"><input placeholder="快速查询" data-event-opts="{{[['input',[['get_wd',['$event']]]]]}}" value="{{my_search_value}}" bindinput="__e"/><image src="../../static/images/sweep.png" data-event-opts="{{[['tap',[['is_scan',['$event']]]]]}}" bindtap="__e"></image></view><button size="mini" data-event-opts="{{[['tap',[['is_search',['$event']]]]]}}" bindtap="__e">搜索</button></view><view class="top p20"><view class="top_main p24"><text class="user">{{"处理人员："+user.username}}</text><text>{{"当前共"+count+"件"}}</text></view></view><block wx:if="{{select!=4&&select!=5}}"><view class="list"><block wx:for="{{my_data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="title">{{item.time}}</view><view class="item_top p24"><view class="item_top_left"><text>瑕疵图片</text></view><view class="id">{{"ID :"+item.bar_id}}</view></view><view class="imgs"><scroll-view style="white-space:nowrap;" scroll-x="true" enable-flex="true"><block wx:for="{{item.original_image}}" wx:for-item="ite" wx:for-index="idx" wx:key="idx"><view class="img_item"><image class="icon" data-list="{{item.original_image}}" data-index="{{idx}}" src="{{ite+'-thumb'}}" data-event-opts="{{[['tap',[['bigImg',['$event']]]]]}}" bindtap="__e"></image></view></block></scroll-view></view><view class="item_footer p24"><view class="price">{{"处理费用 (￥"+item.price+")"}}</view><view class="price_info"><text>{{item.scheme===null?'暂无方案':item.scheme}}</text></view></view><view class="handle_after p24"><view class="handle_after_title">处理后图片</view><view class="imgs"><scroll-view style="white-space:nowrap;" scroll-x="true" enable-flex="true"><block wx:for="{{item.end_image}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="img_item"><image class="icon" data-list="{{item.end_image}}" data-index="{{index}}" src="{{item+'-thumb'}}" data-event-opts="{{[['tap',[['bigImg',['$event']]]]]}}" bindtap="__e"></image></view></block></scroll-view></view></view></view></block><block wx:if="{{!my_data.length}}"><view class="wu">暂无数据</view></block></view></block></view>