<view class="container"><scroll-view class="nav" style="white-space:nowrap;" scroll-x="true"><block wx:for="{{nav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['nav_item',select==item.id?'active':'']}}" data-selectIndex="{{index}}" data-index="{{item.id}}" data-page="{{item.page}}" data-event-opts="{{[['tap',[['change_nav',['$event']]]]]}}" bindtap="__e"><view>{{''+item.name+''}}<block wx:if="{{item.num>0}}"><text>{{"("+item.num+")"}}</text></block></view></view></block></scroll-view><view class="list"><block wx:if="{{!data[select].length}}"><view class="no-data"><text>暂无数据</text></view></block><block wx:for="{{data[select]}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="item_top p24"><view class="item_top_left"><text>瑕疵图片</text></view><view class="id">{{"ID :"+item.bar_id}}</view></view><view class="imgs"><scroll-view style="white-space:nowrap;" scroll-x="true" enable-flex="true"><block wx:for="{{item.original_image}}" wx:for-item="itm" wx:for-index="idx" wx:key="idx"><view class="img_item" data-list="{{item.original_image}}" data-index="{{idx}}" data-event-opts="{{[['tap',[['bigImg',['$event']]]]]}}" bindtap="__e"><image class="icon" src="{{itm+'-thumb'}}"></image></view></block></scroll-view></view><view class="item_footer p24"><view class="price">{{"处理费用 (￥"+item.price+")"}}</view></view><view class="price_info"><text>{{item.scheme===null?'暂无方案':item.scheme}}</text><text>{{item.created_at}}</text></view><view class="handle_after p24"><view class="handle_after_title">处理后图片</view><view class="imgs"><scroll-view style="white-space:nowrap;" scroll-x="true" enable-flex="true"><block wx:for="{{item.end_image}}" wx:for-item="it" wx:for-index="idx" wx:key="idx"><view class="img_item" data-list="{{item.end_image}}" data-index="{{idx}}" data-event-opts="{{[['tap',[['bigImg',['$event']]]]]}}" bindtap="__e"><image class="icon" src="{{it+'-thumb'}}"></image></view></block></scroll-view></view></view></view></block></view></view>