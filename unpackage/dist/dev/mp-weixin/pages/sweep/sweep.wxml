<view class="sweep"><view class="uni-common-mt"><view class="uni-form-item uni-column"><input class="uni-input" focus placeholder="请填写条形码" data-event-opts="{{[['input',[['shapecodeFn',['$event']]]]]}}" value="{{shapecode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['sweepHandler',['$event']]]]]}}" class="sweep-img-box" bindtap="__e"><image class="sweep-img" src="../../static/images/sweep.png"></image></view></view></view><view class="btns"><view class="btn" data-shapecode="{{shapecode}}" data-event-opts="{{[['tap',[['repair',['$event']]]]]}}" bindtap="__e">清洗/修复</view><view data-event-opts="{{[['tap',[['setScheme',['$event']]]]]}}" class="btn" bindtap="__e">设置方案</view></view><view class="btns"><view data-event-opts="{{[['tap',[['repairSuc',['$event']]]]]}}" class="btn" bindtap="__e">修复完成</view><view data-event-opts="{{[['tap',[['myHandler',['$event']]]]]}}" class="btn" bindtap="__e">我的清洗</view></view><view class="btns"><view data-event-opts="{{[['tap',[['noRepair',['$event']]]]]}}" class="btn" style="opacity:0;" bindtap="__e">复查/返工</view><view class="btn" style="opacity:0;">发现问题</view></view></view>